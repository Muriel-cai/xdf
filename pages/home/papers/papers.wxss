image{
  width:100%;
}

.box{
  padding:30rpx;
}
.tip{
  font-size:14px;
  color:#888;
}
.photoBlock{
  padding:10rpx 0;
}
.photo_title{
  padding:10rpx 0;
  border-bottom:1rpx solid #eee;
}
.photo_title text{
  padding-left:20rpx;
  border-left:10rpx solid #0097FF;
  line-height:60rpx;
}
.photo_cnt{
  display:flex;
  flex-wrap:wrap;
  justify-content:space-between;
  padding:20rpx 0;
}
.photo_cnt_list{
  width:330rpx;
  height:210rpx;
  margin-top:20rpx;
}
.photo_cnt_list_img{
  width:100%;
  height:100%;
}


.camera{
  position:fixed;
  z-index:1;
  top:80rpx;
  right:0;
  bottom:0;
  left:0;
  background-color:#888;
}
.camera camera{
  width:100%;
  height:90%;
}
.camera_back,.camera_btn{
  position:absolute;
  bottom:3%;
  height:5%;
  color:#fff;
}
.camera_back{
  left:20rpx;
  width:100rpx;
  text-align:left;
}
.camera_btn{
  left:50%;
  width:200rpx;
  margin-left:-100rpx;
  text-align:center;
}
.camera_back image,.camera_btn image{
  width:100%;
  height:100%;
}


.look{
  position:fixed;
  z-index:1;
  top:80rpx;
  right:0;
  bottom:0;
  left:0;
  background-color:#888;
}
.look_tip{
  position:fixed;
  top:0;
  left:0;
  right:0;
  height:80rpx;
  line-height:80rpx;
  padding:0 20rpx;
  color:#fff;
  text-align:center;
  background-color:#888;
}
.look_image{
  width:100%;
  height:90%;
}
.camera_back,.camera_btn{
  position:absolute;
  bottom:3%;
  height:5%;
  color:#fff;
}
.camera_back{
  left:20rpx;
  width:100rpx;
  text-align:left;
}
.camera_btn{
  left:50%;
  width:200rpx;
  margin-left:-100rpx;
  text-align:center;
}
.camera_back image,.camera_btn image{
  width:100%;
  height:100%;
}